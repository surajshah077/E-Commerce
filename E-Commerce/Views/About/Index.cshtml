@model List<About>
<table class="table">
    <thead>
        <tr>
            <th scope="col">AboutTitle1</th>
            <th scope="col">AboutTitle2</th>
            <th scope="col">Image</th>
            <th scope="col">Message</th>
            <th scope="col">phoneNumber</th>
            <th scope="col">shortDescription</th>
            <th scope="col">description</th>
            <th scope="col">Image2</th>
            <th scope="col">Video</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var obj in Model)
        {
            <tr>
                <td>@obj.AboutTitle1</td>
                <td>@obj.AboutTitle2</td>
                <td><img src ="~/@obj.Image" style="height:100px"width="100px"></td>
                <td>@obj.Message</td>
                <td>@obj.phoneNumber</td>
                <td>@obj.shortDescription</td>
                <td>@obj.description</td>
                <td><img src="~/@obj.Image2"style="height:100px"width="100px"></td>
                <td> <video src="~/@obj.Video"style="height:100px"width="100px"/> </td>
                <td>
                    <a asp-action="Edit" asp-controller="About" asp-route-id="@obj.AboutId">Edit</a>
                </td>
            </tr>
        }
     </tbody>
</table>